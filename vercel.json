{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "framework": "node",
  "routes": [{ "src": "/(.*)", "dest": "index.js" }],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "dist/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "regions": ["bom1"]
}
